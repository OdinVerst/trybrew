{
  "trailingSlash": false,
  "redirects": [
    {
      "source": "/(aeropress|cezve|chemex|coldbrew|pourover|summer)/(.*)",
      "destination": "/ru/recipes/$1/$2"
    },
    {
      "source": "/(ru|en)/(aeropress|cezve|chemex|coldbrew|pourover|summer)/(.*)",
      "destination": "/$1/recipes/$2/$3"
    },
    {
      "source": "/(ru|en)/recipes",
      "destination": "/$1/"
    },
    {
      "source": "/",
      "destination": "/ru/"
    }
  ]
}
