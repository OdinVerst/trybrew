{
  "rewrites": [
    {
      "source": "/(.*)",
      "has": [
        {
          "type": "header",
          "key": "Accept-Language",
          "value": "en"
        }
      ],
      "statusCode": 404,
      "destination": "/en/not-found"
    },
    {
      "source": "/(.*)",
      "has": [
        {
          "type": "header",
          "key": "Accept-Language",
          "value": "ru"
        }
      ],
      "statusCode": 404,
      "destination": "/ru/not-found"
    }
  ]
}
